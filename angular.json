{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "cli": {
        "defaultCollection": "@nativescript/schematics"
    },
    "projects": {
        "beancount": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "bc",
            "schematics": {
                "@nativescript/schematics:component": {
                    "styleext": "scss"
                }
            }
        }
    },
    "defaultProject": "beancount"
}
